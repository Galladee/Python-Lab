for i in range(5,0,-1):
    print(" "*(5-i),end=" ")
    for j in range(i):
        print(i, end=" ")
    print(" ")




for i in range(5,0,-1):
    print(" " *(5-i) ,end=" ")
    for j in range(i):
        print(5-i+1 ,end=" ")
    print(" ")




for i in range(1,6):
    print(" " *(5-i), end=" ")
    for j in range(1,i+1):
        print(j, end=" ")
    print(" ")



for i in range(1,6):
    for j in range(1,i+1):
        print(i ,end=" ")
    print(" ")
